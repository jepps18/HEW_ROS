<gazebo>
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
      <robotNamespace>/hew</robotNamespace>
      <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
    </plugin>
  </gazebo>

<link
    name="base_link">
    <inertial>
      <origin
        xyz="0.16541 0.25404 -0.094006"
        rpy="0 0 0"/>
      <mass
        value="2.4395" />
      <inertia
        ixx="0.021816"
        ixy="1.442E-06"
        ixz="1.7046E-09"
        iyy="0.033317"
        iyz="1.9747E-08"
        izz="0.054717" />
    </inertial>

    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://hew/urdf/meshes/base_link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>

    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://hew/urdf/meshes/base_link_col.STL" />
      </geometry>
    </collision>
  </link>

  <link name="world"/>
  <joint name="fixed" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin
        xyz="0 0 1"
        rpy="0 3.14 0" />
  </joint>

    <link
    name="Arm_Link">
    <inertial>
      <origin
        xyz="-0.054863 0.00021536 0.092238"
        rpy="0 0 0" />
      <mass
        value="0.19866" />
      <inertia
        ixx="0.0001194"
        ixy="6.1121E-07"
        ixz="-2.2554E-05"
        iyy="0.00028255"
        iyz="7.9362E-08"
        izz="0.00020139" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://hew/urdf/meshes/Arm_Link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://hew/urdf/meshes/Arm_Link_col.STL" />
      </geometry>
    </collision>
  </link>

  <joint
    name="joint1"
    type="continuous"> <!-- Type was revolute-->
      <origin
      xyz="0.31115 0.254 -0.084782"
      rpy="3.1416 0 0" />
    <parent
      link="base_link" />
    <child
      link="Arm_Link" />
    <axis
      xyz="0 0 1" />
      <dynamics damping="0.075"/>
  </joint>

  <link
    name="Horz_Trans">
    <inertial>
      <origin
        xyz="0.034998 -2.4617E-05 0.00021996"
        rpy="0 0 0" />
      <mass
        value="0.015733" />
      <inertia
        ixx="1.3497E-06"
        ixy="-6.6393E-10"
        ixz="1.622E-08"
        iyy="6.4569E-06"
        iyz="5.1067E-10"
        izz="6.2659E-06" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://hew/urdf/meshes/Horz_Trans.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.75294 0.75294 0.75294 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://hew/urdf/meshes/Horz_Trans_col.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="joint2"
    type="continuous">
    <origin
      xyz="-0.10387 0.00038259 -0.0853"
      rpy="0 0 -0.0036833" />
    <parent
      link="Arm_Link" />
    <child
      link="Horz_Trans" />
    <axis
      xyz="1 0 0" />
      <dynamics damping="0.0001" />
  </joint>
  
  <transmission name="tran2">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint2">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor2">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

<transmission name="tran1">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint1">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor1">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>
   


