    <link
    name="Arm_Link">
    <inertial>
      <!--origin
        xyz="-0.054863 0.00021536 0.092238"
        rpy="0 0 0" /-->
      <origin
        xyz="0 0 1"
        rpy="0 0 0" />
      <mass
        value="0.19866" />
      <inertia
        ixx="0.0001194"
        ixy="6.1121E-07"
        ixz="-2.2554E-05"
        iyy="0.00028255"
        iyz="7.9362E-08"
        izz="0.00020139" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://hew/urdf/meshes/Arm_Link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://hew/urdf/meshes/Arm_Link_col.STL" />
      </geometry>
    </collision>
  </link>

  <link name="world"/>
  <joint
    name="Arm_Link_cont"
    type="revolute">
    <origin
      xyz="0.31115 0.254 1"
      rpy="0 0 0" />
    <parent
      link="world" />
    <child
      link="Arm_Link" />
    <axis
      xyz="0 0 -1" />
    <limit
      lower="1.745"
      upper="-1.745"
      effort="0"
      velocity="20" />
  </joint>
  
    <transmission name="arm_tran">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="Arm_Link_cont">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor1">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

